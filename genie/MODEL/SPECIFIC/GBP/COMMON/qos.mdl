module[qos]
 {
     #Burst and Rate are always in Kbps.
 
     class[BandwidthLimit;
           super=policy/NamedDefinition;
           concrete]
     {
         member[burst; type=scalar/UInt64]
         member[rate; type=scalar/UInt64]

         contained
         {
             parent[class=policy/Space]
         }

         named
         {
             parent[class=*;]
             {
                 component[prefix=bwlimit;
                           member=name]
             }
         }
     }

    class[DscpMarking;
          concrete]
    {
         member[mark; type=scalar/UByte]
         contained
        {
            parent[class=policy/Requirement]
        }

        named
        {
            parent[class=*;]
            {
                component[prefix=dscp-marking;]
            }
        }
    }

     class[Requirement;
           super=policy/NamedDefinition;
           concrete]
     {
         contained
         {
             parent[class=policy/Space]
         }

         named
         {
             parent[class=*;]
             {
                 component[prefix=requirement;
                           member=name]
             }
         }

         dependency[ingress;
              cardinality=single;
              ]
         {
             to[class=qos/BandwidthLimit;
                 cardinality=many;
                 ]
         }

         dependency[egress;
              cardinality=single;
              ]
         {
             to[class=qos/BandwidthLimit;
                 cardinality=many;
                 ]
         }
     }
 }
